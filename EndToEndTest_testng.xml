<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Tools QA">
	<test name="Create User">
		<classes>
			<class name="ToolsQA.RestApi.tests.CreateUser">
				<methods>
					<include name="createUserToAccess">
					</include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Generate Token">
		<classes>
			<class name="ToolsQA.RestApi.tests.GenerateToken">
				<methods>
					<include name="generateToken">
					</include>
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="Authorized user">
		<classes>
			<class name="ToolsQA.RestApi.tests.UserAuthorized">
				<methods>
					<include name="authorizedUser">
					</include>
				</methods>
			</class>
		</classes>
	</test>
	
</suite>
